  <div class="spec-wrapper2">
    <%= f.select :type, [["Density at 15C", "Specs::Density"],
                        ["Viscosity at 40C", "Specs::Viscosity"],
                        ["Flash point at 40C", "Specs::Flash_point"],
                        ["Pour point", "Specs::Pour_point"],
                        ["Sulfur", "Specs::Sulfur"],
                        ["Cetane index", "Specs::Cetane_index"],
                        ["Carbon residue", "Specs::Carbon_residue"],
                        ["Ash %", "Specs::Ash"],
                        ["Water", "Specs::Water"],
                        ["Sulfur", "Specs::Vanadium"],
                        ["Aluminum + silicon", "Specs::Aluminum_plus_silicon"],
                        ["Zinc", "Specs::Zinc"],
                        ["Phosphorus", "Specs::Phosphorus"],
                        ["Calcium", "Specs::Calcium"]] %>
    </select>
  </div>

  <div class="spec-wrapper3">
    <%= f.text_field :unit, placeholder: "ex. Celcius" %>
  </div>

  <div class="spec-wrapper4">
    <%= f.text_field :limit, placeholder: "max./min." %>
  </div>

  <div class="spec-wrapper5">
    <%= f.text_field :value, placeholder: "ex. 3.50" %>
    <%= f.hidden_field :type %>
  </div>

  <div class="spec-wrapper6">
    <%= f.text_field :test_method_reference, placeholder: "ex. ISO 3016" %>
  </div>
<br>

  <div class="spec-wrapper2">
    <%= f.text_field :name, :required => true,  placeholder: "Characteristic (ex. Sulfur)*" %>
  </div>

  <div class="spec-wrapper3">
    <%= f.text_field :unit, :required => true,  placeholder: "Unit (ex. kg/m3, â„ƒ, etc.)*" %>
  </div>

  <div class="spec-wrapper4">
    <%= f.text_field :limit, :required => true,  placeholder: "max./min.*" %>
  </div>

  <div class="spec-wrapper5">
    <%= f.text_field :value, :required => true,  placeholder: "Value (ex. 3.50)*" %>
    <%= f.hidden_field :type %>
  </div>

  <div class="spec-wrapper6">
    <%= f.text_field :test_method_reference, :required => true,  placeholder: "Test Method (ex. ISO 3016)*" %>
  </div>

